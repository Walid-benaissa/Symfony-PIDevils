{% extends 'base-back.html.twig'%}

{% block body %}
	<h1>Vehicule</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>IdVehicule</th>
				<td>{{ vehicule.idVehicule }}</td>
			</tr>
			<tr>
				<th>NomV</th>
				<td>{{ vehicule.nomV }}</td>
			</tr>
			<tr>
				<th>Id</th>
				<td>{{ vehicule.id }}</td>
			</tr>
			<tr>
				<th>Image</th>
				<td>{{ vehicule.image }}</td>
			</tr>
			<tr>
				<th>Ville</th>
				<td>{{ vehicule.ville }}</td>
			</tr>
			<tr>
				<th>Prix</th>
				<td>{{ vehicule.prix }}</td>
			</tr>
			<tr>
				<th>Disponibilite</th>
				<td>{{ vehicule.disponibilite ? 'Yes' : 'No' }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ vehicule.description }}</td>
			</tr>
			<tr>
				<th>Type</th>
				<td>{{ vehicule.type }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_vehicule_index') }}">back to list</a>

	<a href="{{ path('app_vehicule_edit', {'idVehicule': vehicule.idVehicule}) }}">edit</a>

	{{ include('vehicule/_delete_form.html.twig') }}
{% endblock %}
