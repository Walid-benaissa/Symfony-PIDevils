<form method="post" action="{{ path('app_voiture_delete', {'immatriculation': voiture.immatriculation}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ voiture.immatriculation) }}">
    <button class="submit" style="width: 120px; height: 40px text-align:center">Supprimer</button>
</form>
