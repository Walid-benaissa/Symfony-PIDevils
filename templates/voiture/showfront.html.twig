{% extends 'base-front.html.twig'%} 
{% block body %}

<div>
<title>Voiture</title>
    <h1>Voiture</h1>
    {% if voiture != null %}
    <table class="table" >
        <tbody>
            <tr>
                <th>Immatriculation</th>
                <td>{{ voiture.immatriculation }}</td>
            </tr>
            <tr>
                <th>Modele</th>
                <td>{{ voiture.modele }}</td>
            </tr>
            <tr>
                <th>Marque</th>
                <td>{{ voiture.marque }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ voiture.etat }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ voiture.photo }}</td>
            </tr>
        </tbody>
    </table>
    <br>
    {{ include('voiture/_delete_form.html.twig') }}
    {% else %}
    <h1>aucune voiture existe</h1>
    {% endif %}
    <div>
    <br>
{% endblock %}
